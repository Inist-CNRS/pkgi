<?php echo '#!/bin/sh'; ?> 
if [ "x$1" = "x" -o "x$1" = "x-h" -o "x$1" = "x--help" ]; then
	exec <?php echo getenv('APPNAME_HOME') ?>/usr/src/sphinx/src/spelldump
else 
	exec <?php echo getenv('APPNAME_HOME') ?>/usr/src/sphinx/src/spelldump -c <?php echo getenv('APPNAME_HOME') ?>/etc/sphinx/sphinx.conf $*
fi
